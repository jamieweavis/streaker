{
  "name": "streaker",
  "productName": "Streaker",
  "private": true,
  "description": "Cross-platform GitHub contribution streak and statistic tracking menu bar application with reminder notification",
  "version": "2.6.1",
  "homepage": "https://github.com/jamieweavis/streaker#readme",
  "license": "MIT",
  "main": ".webpack/main",
  "author": "Jamie Weavis",
  "packageManager": "pnpm@10.18.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamieweavis/streaker.git"
  },
  "bugs": {
    "url": "https://github.com/jamieweavis/streaker/issues"
  },
  "keywords": [
    "contribution",
    "contribution-graph",
    "electron",
    "electron-app",
    "github",
    "macos",
    "menubar",
    "streak"
  ],
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "check": "biome check src",
    "check:write": "pnpm check -- --write"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/commit-analyzer",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/release-notes-generator"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@electron-forge/cli": "7.10.2",
    "@electron-forge/maker-deb": "7.10.2",
    "@electron-forge/maker-dmg": "7.10.2",
    "@electron-forge/maker-rpm": "7.10.2",
    "@electron-forge/maker-squirrel": "7.10.2",
    "@electron-forge/maker-zip": "7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "7.10.2",
    "@electron-forge/plugin-fuses": "7.10.2",
    "@electron-forge/plugin-webpack": "7.10.2",
    "@electron-forge/publisher-github": "7.10.2",
    "@electron/fuses": "2.0.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.0",
    "@semantic-release/npm": "13.1.1",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/auto-launch": "5.0.5",
    "@vercel/webpack-asset-relocator-loader": "1.10.0",
    "css-loader": "7.1.2",
    "electron": "38.3.0",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "node-loader": "2.1.0",
    "postcss-loader": "8.2.0",
    "semantic-release": "25.0.1",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@tailwindcss/postcss": "4.1.15",
    "auto-launch": "5.0.6",
    "contribution": "7.1.2",
    "cron": "4.3.3",
    "electron-log": "5.4.3",
    "electron-squirrel-startup": "1.0.1",
    "electron-store": "8.2.0",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.15",
    "update-electron-app": "3.1.1"
  }
}
