{
  "name": "streaker",
  "productName": "Streaker",
  "private": true,
  "description": "Cross-platform GitHub contribution streak/statistic tracking menu bar application with reminder notification",
  "version": "2.6.1",
  "homepage": "https://github.com/jamieweavis/streaker#readme",
  "license": "MIT",
  "main": ".webpack/main",
  "author": "Jamie Weavis",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamieweavis/streaker.git"
  },
  "bugs": {
    "url": "https://github.com/jamieweavis/streaker/issues"
  },
  "keywords": [
    "contribution",
    "contribution-graph",
    "electron",
    "electron-app",
    "github",
    "macos",
    "menubar",
    "streak"
  ],
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "check": "biome check src",
    "check:write": "pnpm check -- --write"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/commit-analyzer",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/release-notes-generator"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-deb": "^7.7.0",
    "@electron-forge/maker-dmg": "^7.7.0",
    "@electron-forge/maker-rpm": "^7.7.0",
    "@electron-forge/maker-squirrel": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.7.0",
    "@electron-forge/plugin-fuses": "^7.7.0",
    "@electron-forge/plugin-webpack": "^7.7.0",
    "@electron-forge/publisher-github": "^7.7.0",
    "@electron/fuses": "^1.8.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@types/auto-launch": "^5.0.5",
    "@vercel/webpack-asset-relocator-loader": "^1.7.4",
    "css-loader": "^7.1.2",
    "electron": "35.7.5",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "node-loader": "^2.1.0",
    "postcss-loader": "^8.1.1",
    "semantic-release": "24.2.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@tailwindcss/postcss": "4.0.14",
    "auto-launch": "^5.0.6",
    "contribution": "^7.0.0",
    "cron": "^4.1.0",
    "electron-log": "^5.3.2",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.2.0",
    "postcss": "^8.5.3",
    "tailwindcss": "4.0.14",
    "update-electron-app": "^3.1.1"
  }
}
